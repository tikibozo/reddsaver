    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'reddsaver'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=reddsaver",
                    "--package=reddsaver"
                ],
                "filter": {
                    "name": "reddsaver",
                    "kind": "bin"
                }
            },
            "args": [
                "-e","/Users/jon/reddsaver/reddsaver.env",
                "-d","/Users/jon/reddsaver/data"],
            "cwd": "${workspaceFolder}"
        }
    ]
}